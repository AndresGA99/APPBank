<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fondo_2"
    tools:context=".MenuActivity">

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="408dp"
        android:layout_height="71dp"
        android:layout_marginTop="130dp"
        android:layout_marginEnd="3dp"
        android:layout_marginBottom="2dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnDeposito"
        app:layout_constraintVertical_bias="0.098"
        app:srcCompat="@drawable/logo" />

    <Button
        android:id="@+id/btnRetiros"
        android:layout_width="240dp"
        android:layout_height="52dp"
        android:layout_marginTop="412dp"
        android:layout_marginBottom="8dp"
        android:backgroundTint="#C500FF"
        android:fontFamily="@font/bebas_neue"
        android:text="Retiros"
        android:textColorLink="#9C27B0"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/btnDeposito"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnDeposito"
        android:layout_width="240dp"
        android:layout_height="52dp"
        android:layout_marginTop="472dp"
        android:layout_marginBottom="8dp"
        android:backgroundTint="#C500FF"
        android:fontFamily="@font/bebas_neue"
        android:text="Depositar saldo"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/btnconsulta"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.502"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnconsulta"
        android:layout_width="240dp"
        android:layout_height="52dp"
        android:layout_marginTop="532dp"
        android:layout_marginBottom="12dp"
        android:backgroundTint="#C500FF"
        android:fontFamily="@font/bebas_neue"
        android:text="Consultas"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/btnTrans"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnTrans"
        android:layout_width="240dp"
        android:layout_height="52dp"
        android:layout_marginTop="596dp"
        android:layout_marginBottom="14dp"
        android:backgroundTint="#C500FF"
        android:fontFamily="@font/bebas_neue"
        android:text="Transferencias"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="149dp"
        android:layout_height="36dp"
        android:layout_marginTop="28dp"
        android:layout_marginBottom="228dp"
        android:text="Bienvenido"
        android:textColor="#FFFFFF"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.061"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="111dp"
        android:layout_height="31dp"
        android:fontFamily="@font/bebas_neue"
        android:text="Opciones"
        android:textSize="24sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="350dp" />



    <Button
        android:id="@+id/btnSalir"
        android:layout_width="200dp"
        android:layout_height="51dp"
        android:layout_marginStart="104dp"
        android:layout_marginTop="40dp"
        android:backgroundTint="#FF3030"
        android:text="Salir"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnDeposito" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="120dp"
        android:layout_height="165dp"
        android:layout_marginStart="257dp"
        android:layout_marginTop="152dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/av_logo" />


</androidx.constraintlayout.widget.ConstraintLayout>