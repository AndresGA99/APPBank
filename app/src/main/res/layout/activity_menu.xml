<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="match_parent"
    tools:context=".MenuActivity">


    <FrameLayout
        android:id="@+id/frame_layout"
        android:layout_width="396dp"
        android:layout_height="719dp"
        app:layout_constraintBottom_toTopOf="@+id/bottomNavigationView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0">

    </FrameLayout>

    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/bottomNavigationView"
        android:layout_width="395dp"
        android:layout_height="58dp"
        android:layout_marginTop="668dp"
        app:backgroundTint="#C6C6C6"
        app:circularflow_defaultRadius="8sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:itemIconSize="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.530"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.99"
        app:itemIconPadding="80dp"
        app:menu="@menu/bottom_nav" />

    <Button
        android:id="@+id/btnRetiros"
        android:layout_width="240dp"
        android:layout_height="52dp"
        android:layout_marginTop="412dp"
        android:layout_marginBottom="8dp"
        android:backgroundTint="#C500FF"
        android:fontFamily="@font/bebas_neue"
        android:text="Retiros"
        android:textColorLink="#9C27B0"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/btnDeposito"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnSalir"
        android:layout_width="90dp"
        android:layout_height="38dp"
        android:layout_marginStart="292dp"
        android:layout_marginEnd="29dp"
        android:backgroundTint="#673AB7"
        android:text="Salir"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.023" />

    <Button
        android:id="@+id/btnDeposito"
        android:layout_width="240dp"
        android:layout_height="52dp"
        android:layout_marginTop="472dp"
        android:layout_marginBottom="8dp"
        android:backgroundTint="#C500FF"
        android:fontFamily="@font/bebas_neue"
        android:text="Depositar saldo"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/btnconsulta"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.502"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnconsulta"
        android:layout_width="240dp"
        android:layout_height="52dp"
        android:layout_marginTop="532dp"
        android:layout_marginBottom="12dp"
        android:backgroundTint="#C500FF"
        android:fontFamily="@font/bebas_neue"
        android:text="Consultas"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/btnTrans"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnTrans"
        android:layout_width="240dp"
        android:layout_height="52dp"
        android:layout_marginTop="596dp"
        android:layout_marginBottom="14dp"
        android:backgroundTint="#C500FF"
        android:fontFamily="@font/bebas_neue"
        android:text="Transferencias"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/bottomNavigationView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="246dp"
        android:layout_height="55dp"
        android:layout_marginStart="82dp"
        android:layout_marginTop="68dp"
        android:layout_marginEnd="83dp"
        android:fontFamily="@font/bebas_neue"
        android:text="Bienvenido"
        android:textAlignment="center"
        android:textColor="#1A1919"
        android:textSize="60sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="111dp"
        android:layout_height="31dp"
        android:layout_marginStart="27dp"
        android:layout_marginTop="238dp"
        android:layout_marginEnd="255dp"
        android:layout_marginBottom="24dp"
        android:fontFamily="@font/bebas_neue"
        android:text="Opciones"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/btnRetiros"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView9"
        app:layout_constraintVertical_bias="1.0" />

</androidx.constraintlayout.widget.ConstraintLayout>